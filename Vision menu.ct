              <AssemblerScript>[ENABLE]
{$lua}
function infAmmo(state)
   local ptr = readPointer(readPointer("[WorldPTR]+pCPed")+0x10D0)
   if not ptr then return end
   local ammoList = readPointer(ptr + 0x48)
   local ammoNum = readSmallInteger(ptr + 0x50)
   if ammoNum == 0 then return end
   local count = ammoNum - 1
   for i = 0,count,1 do
      local ammo = readPointer(ammoList + 0x8 * i)
      if not ammo then goto continue end
      writeBytes(ammo + 0x24, state)
      ::continue::
   end
end
if infammoTimer == nil then
   infammoTimer = createTimer(nil,false)
   infammoTimer.Interval = 10000
   infammoTimer.OnTimer = function() infAmmo(1) end
end
