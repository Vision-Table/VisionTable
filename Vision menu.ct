                  <LastState/>
                  <VariableType>Auto Assembler Script</VariableType>
                  <AssemblerScript>{$lua}
local addr = readPointer("[[[[WorldPTR]+pCPed]+pCVehicle]+pCVehicleDrawHandler]+pCVehicleVisual")
if addr then
  local input,new = inputQuery('New License Plate','',''),{}
  if input == nil or input == '' then return end
  input:gsub(".",function(c) table.insert(new,c) end)
  for k,v in pairs(new) do
    if tonumber(v) == nil then
    v = string.byte(v)-87
    end
    writeBytes(addr+oLicensePlate+k-1,v)
  end
end

[ENABLE]
[DISABLE]
