        elseif hash==joaat("Voltic2") or hash==joaat("Oppressor") or hash==joaat("Vigilante") then
      if isKeyPressed(VK_E) and boost_level &lt; 1.24 then writeFloat(boostAdr, 1.25) end
   else return end
  end
